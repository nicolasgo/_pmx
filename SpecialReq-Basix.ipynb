      "import hashlib\n",
      "\n",
      "saved = {}\n",
      "rsaved = {}  # a reverse dictionary can be used to lookup original msisdn from hashed ids\n",
      "\n",
      "\n",
      "def xx(x):\n",
      "    if not saved.has_key(x):\n",
      "        try:\n",
      "            m = hashlib.md5()\n",
      "            m.update(x)\n",
      "            mh = '%.12s'%(m.hexdigest()[10:30])\n",
      "            saved[x]=mh\n",
      "            rsaved[mh]=x\n",
      "\n",
      "\n",
      "        except:\n",
      "            print type(x),x\n",
      "\n",
      "#        print x,mh\n",
      "\n",
      "    return saved[x]\n",
      "\n",
      "\n",
